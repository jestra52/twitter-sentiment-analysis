{
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "name": "twt-sentiment-analysis-web",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@material-ui/core": "4.0.0-rc.0",
    "@material-ui/icons": "^4.0.0-rc.0",
    "axios": "0.21.1",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-jest": "21.22.0",
    "npm-run-all": "4.1.3",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "eject": "react-scripts eject",
    "lintj": "./node_modules/.bin/eslint --ext .js src/",
    "lints": "./node_modules/.bin/stylelint \"src/**/*.scss\"",
    "start": "cross-env REACT_APP_ENV=local npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  }
}
